{
  "name": "backend",
  "version": "1.0.0",
  "main": "index.js",
  "scripts": {
    "start": "node index.js",
    "dev": "node index.js",
    "test": "jest",
    "test:contract": "CONTRACT_ONLY=1 jest --runInBand tests/*.contract.test.js",
    "test:ci": "SKIP_DB=1 jest --runInBand",
    "test:watch": "jest --watchAll",
    "lint": "eslint . --ext .js",
    "lint:strict": "eslint . --ext .js --max-warnings=0",
    "lint:openapi": "redocly lint openapi.yaml",
    "lint:error-codes": "node scripts/lintErrorCodes.js",
    "gen:operation-ids": "node scripts/generateOperationIds.js",
    "gen:types": "npx openapi-typescript openapi.yaml --output ../frontend/src/generated/schema.d.ts",
    "seed:bikes": "node scripts/seedBikes.js",
    "purge:bikes": "node scripts/purgeDeletedBikes.js",
    "migrate:bikes:ownerEmailLower": "node scripts/migrateOwnerEmailLower.js",
    "diff:coverage": "node scripts/diffCoverage.js --threshold 80"
  },
  "keywords": [],
  "author": "",
  "description": "",
  "dependencies": {
    "bcryptjs": "^3.0.2",
    "cors": "^2.8.5",
    "dotenv": "^17.2.1",
    "express": "^5.1.0",
    "express-rate-limit": "^8.0.1",
    "express-validator": "^7.2.1",
    "jsonwebtoken": "^9.0.2",
    "mongoose": "^8.17.1",
    "nodemailer": "^7.0.5",
    "multer": "^1.4.5-lts.2",
    "helmet": "^8.1.0",
    "swagger-ui-express": "^5.0.1",
    "yaml": "^2.5.0",
    "prom-client": "^15.1.3"
  },
  "devDependencies": {
    "pino-pretty": "^11.2.2",
    "jest": "^30.0.5",
    "supertest": "^7.1.4",
    "jest-openapi": "^0.14.2",
    "@redocly/cli": "^1.28.6",
    "mongodb-memory-server": "^10.1.4"
  },
  "optionalDependencies": {
    "pino": "^9.3.2"
  }
}
